                     qc_test_vert_consistency , qc_test_convective_adj , &
!BPR BEGIN
                     qc_psfc , &
!BPR END
                     max_error_t      , max_error_uv   , &
                     max_error_z      , max_error_rh   , &
!BPR BEGIN
                     max_error_dewpoint ,                 &
!BPR END
                     max_error_p      , print_error_max , &
                     max_buddy_t      , max_buddy_uv   , &
                     max_buddy_z      , max_buddy_rh   , &
!BPR BEGIN
                     max_buddy_dewpoint ,                &
!BPR END
!BPR BEGIN
