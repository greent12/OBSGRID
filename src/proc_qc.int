   INTEGER , INTENT ( IN )                    :: iew, jns, kmax ,    &
                                                 number_of_obs ,     &
                                                 total_dups ,        &
                                                 map_projection
   LOGICAL , INTENT ( IN )                    :: qc_test_error_max         , &
                                                 qc_test_buddy             , &
                                                 qc_test_vert_consistency  , &
!BPR BEGIN
!                                                qc_test_convective_adj
                                                 qc_test_convective_adj    , &
                                                 qc_psfc
!BPR END
   REAL    , INTENT ( IN )                    :: max_error_p,             &
                                                 max_error_t,             &
                                                 max_error_uv,            &
                                                 max_error_z,             &
                                                 max_error_rh,            &
!BPR BEGIN
                                                 max_error_dewpoint,      &
!BPR END
                                                 max_buddy_p,             &
                                                 max_buddy_t,             &
                                                 max_buddy_uv,            &
                                                 max_buddy_z,             &
!BPR BEGIN
